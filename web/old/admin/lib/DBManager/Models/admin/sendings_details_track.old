<?php

	class sendings_details_track extends SQLTable {
		function sendings_details_track(&$Con) {
			$this->involvedTables = array('dm_subscribers','dm_tracking');
			$this->_defaultFields = "dm_subscribers.*,dm_tracking.id AS trackid";
			parent::SQLTable($Con,"dm_tracking","dm_tracking LEFT JOIN dm_subscribers ON dm_tracking.subscriber_id=dm_subscribers.id","id");
		}

		function Find($fieldsList = "",$Cond = "",$Order = "",$Limit="", $GroupBy="") {
			if(!$fieldsList) $fieldsList=$this->_defaultFields;
			return parent::Find($fieldsList,$Cond,$Order,$Limit,$GroupBy);	
		}
	}
?>
{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.5.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/serializer-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "1.3.*",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "friendsofsymfony/jsrouting-bundle": "@stable",

        "stof/doctrine-extensions-bundle": "~1.1@dev",

        "knplabs/knp-menu-bundle": "~2.0",
        "knplabs/knp-paginator-bundle": "~2.4",

        "twbs/bootstrap": "dev-master",
        "jquery/jquery":  "1.11.*",
        "jquery/jquery-ui":  "1.10.*",
        "braincrafted/bootstrap-bundle": "~2.0",
        "bmatzner/fontawesome-bundle": "~4.1",
        "necolas/normalize.css":"dev-master",
        "WickyNilliams/enquire.js":"dev-master",
        "liip/imagine-bundle": "dev-master",
        "suncat/mobile-detect-bundle": "0.9.*",

        "hybridauth/hybridauth": "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.11.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.11.1.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery-ui",
                "version": "1.10.4",
                "dist": {
                    "url": "http://code.jquery.com/ui/1.10.4/jquery-ui.js",
                    "type": "file"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"silviomoreto/bootstrap-select",
                "version":"dev-master",
                "source":{
                    "url":"git://github.com/silviomoreto/bootstrap-select.git",
                    "type":"git",
                    "reference":"origin/master"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"necolas/normalize.css",
                "version":"dev-master",
                "source":{
                    "url":"git://github.com/necolas/normalize.css.git",
                    "type":"git",
                    "reference":"origin/master"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"WickyNilliams/enquire.js",
                "version":"dev-master",
                "source":{
                    "url":"git://github.com/WickyNilliams/enquire.js.git",
                    "type":"git",
                    "reference":"origin/master"
                }
            }
        }
    ]
}
